package com.arif.lookify.models;

import javax.validation.constraints.Size;

public class Lookify {
	@Size(min=5, max=50)
	private String name;
	
	@Size(min=5, max=20)
	private String artist;
	
	@Size(min=1, max=10)
	private double rating;
	
	public Lookify(){}
	
	public Lookify(String name, String artist, double rating){
		this.name = name;
		this.artist = artist;
		this.rating = rating;
	}
}
