<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Pets</display-name>
  <description>Pets application</description>
  <servlet>
    <servlet-name>Home</servlet-name>
    <servlet-class>com.arif.pets.controllers.Home</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/Home</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ShowCat</servlet-name>
    <servlet-class>com.arif.pets.controllers.ShowCat</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowCat</servlet-name>
    <url-pattern>/Animals/ShowCat</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ShowDog</servlet-name>
    <servlet-class>com.arif.pets.controllers.ShowDog</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowDog</servlet-name>
    <url-pattern>/Animals/ShowDog</url-pattern>
  </servlet-mapping>
</web-app>